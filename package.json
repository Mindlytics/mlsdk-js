{
  "name": "mlsdk-js",
  "version": "0.0.2",
  "description": "Mindlytics SDK for JavaScript",
  "scripts": {
    "build": "yarn build:core && yarn build:node-sdk",
    "build:core": "yarn workspace @mindlytics/core build",
    "build:node-sdk": "yarn workspace @mindlytics/node-sdk build",
    "generate:api:schema": "yarn workspace @mindlytics/core generate",
    "test": "vitest",
    "release": "yarn build && yarn changeset publish"
  },
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.29.5",
    "msw": "^2.8.7",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "packageManager": "yarn@4.9.2"
}
